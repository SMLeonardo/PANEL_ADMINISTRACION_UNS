document.getElementById("Noticia.php")

function nuevo_vinculo()
{

$img_vinculo = $archivo;
$desc_vinculo = $descripcion;

agregarv($img_vinculo, $desc_vinculo);

}


function nueva_noticia()
{

$img_noticia = $imagen;
$desc_noticia = $descripcion;

agregarn($img_noticia, $desc_noticia);

}

